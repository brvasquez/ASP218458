
@{
    ViewBag.Title = "uploadCSV";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br>
<br>
<br>
    <h2>Cargar CSV</h2>

    @using (Html.BeginForm("uploadCSV", "Proveedors", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {



        <div class="form-group">
            <label class="control-label col-md-2" for="file">cargar archivo: </label>
            <div class="col-md-10">
                <input type="file" name="fileForm" class="btn btn-default" />
            </div>
        </div>



        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="enviar" class="btn btn-success" />
            </div>
        </div>


        <div>
            @Html.ActionLink("Back to List", "Index")
        </div>
    }

