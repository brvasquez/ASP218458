
@{
    ViewBag.Title = "uploadCSV";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br>
<br>
<br>
<h2>Cargar CSV</h2>

@using (Html.BeginForm("uploadCSV", "Proveedors", FormMethod.Post, new { enctype = "multipart/form-data" }))
{



    <div class="form-group">
        <label class="control-label col-md-2" for="file">cargar archivo: </label>
        <div class="col-md-10">
            <input type="file" name="fileForm" class="btn btn-default" />
        </div>
    </div>

    <br>
    <br>
    <br>
   

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="enviar" class="btn btn-success" />
        </div>
    </div>

    <div class="container">
    <div class="row">
        <div class="col-md-3 col-sm-6">
            <div class="progress blue">
                <span class="progress-left">
                    <span class="progress-bar"></span>
                </span>
                <span class="progress-right">
                    <span class="progress-bar"></span>
                </span>
                <div class="progress-value">90%</div>
            </div>
        </div>
 
        <div class="col-md-3 col-sm-6">
            <div class="progress yellow">
                <span class="progress-left">
                    <span class="progress-bar"></span>
                </span>
                <span class="progress-right">
                    <span class="progress-bar"></span>
                </span>
                <div class="progress-value">75%</div>
            </div>
        </div>
    </div>
</div>


    <div>
        @Html.ActionLink("Back to List", "Index")
    </div>
}

